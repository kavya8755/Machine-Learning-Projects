from multiprocessing import Pool
import os

def if_prime(x):
    if x <= 1:
        return 0
    elif x <= 3:
        return x
    elif x % 2 == 0 or x % 3 == 0:
        return 0
    i = 5
    while i**2 <= x:
        if x % i == 0 or x % (i + 2) == 0:
            return 0
        i += 6
    return x
    
def cpu_use(cpu):
	if cpu == 1:
		return 1
	if cpu > 1 and cpu <= 5:
		if cpu // 2 == cpu /2:
			return cpu//2
		else:
			return cpu//2 + 1
	else:
		return round(cpu * 0.75)

t = time.time()

answer = 0

for i in range(1000000):
    answer += if_prime(i)
print(answer)
print(time.time()-t)

t = time.time()

use_cpu = cpu_use(os.cpu_count())

pool = Pool(processes = 2)
answer = sum(pool.map(if_prime, range(1000000)))
print(answer)
print(time.time()-t)

